<div class="container">
    <section id="main">
      {{#if articles}}
      <h2>Wired</h2>
<div class="row">
   {{#each articles}}
        <div class="col s12 m6">
          <div class="card blue-grey darken-1">
            <div class="card-content white-text">
              <span class="card-title">{{this.title}}</span>
              <p>{{this.body}}</p>
            </div>
            <div class="card-action">
              <a href="#" class="delete" id={{this._id}}>Delete</a>
              <a href="#">Comment</a>
            </div>
          </div>
        </div>
        {{/each}}
      </div>
      {{/if}}
    </section>
  </div>